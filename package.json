{
  "name": "sao",
  "version": "0.22.17",
  "description": "Futuristic project generator.",
  "license": "MIT",
  "repository": "saojs/sao",
  "author": {
    "name": "EGOIST",
    "email": "0x142857@gmail.com",
    "url": "https://github.com/egoist"
  },
  "engines": {
    "node": ">=6"
  },
  "bin": "bin/sao",
  "scripts": {
    "test": "npm run lint && ava",
    "lint": "xo"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin"
  ],
  "keywords": [
    "generator",
    "template",
    "scaffolding",
    "sao"
  ],
  "devDependencies": {
    "ava": "^0.22.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-rem": "^3.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "prettier": "^1.9.2",
    "xo": "^0.18.2"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  },
  "xo": {
    "extends": [
      "prettier",
      "rem"
    ],
    "envs": [
      "jest"
    ],
    "ignores": [
      "docs/**",
      "test/fixture/**",
      "test/output/**"
    ],
    "rules": {
      "guard-for-in": 0,
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "dependencies": {
    "boxen": "^1.2.2",
    "cac": "^4.3.4",
    "chalk": "^2.0.1",
    "co": "^4.6.0",
    "conf": "^1.1.2",
    "cross-spawn": "^5.1.0",
    "download-git-repo": "^1.0.1",
    "filenamify": "^2.0.0",
    "fs-extra": "^4.0.1",
    "git-config-path": "^1.0.1",
    "globby": "^6.1.0",
    "jstransformer-handlebars": "^1.0.0",
    "kopy": "^8.2.0",
    "ora": "^1.3.0",
    "parse-git-config": "^1.1.1",
    "semver": "^5.4.1",
    "text-table": "^0.2.0",
    "tildify": "^1.2.0",
    "update-notifier": "^2.2.0",
    "user-home": "^2.0.0",
    "yarn-install": "^1.0.0"
  }
}
